   "source": [
    "import pandas as pd\n",
    "\n",
    "# Sample data\n",
    "data = {\n",
    "    'Name': ['Alice', 'Bob', 'Charlie', 'David', 'Eve'],\n",
    "    'City': ['Delhi', 'Mumbai', 'Delhi', 'Mumbai', 'Bangalore'],\n",
    "    'Age': [25, 30, 35, 40, 28]\n",
    "}\n",
    "\n",
    "# Create DataFrame\n",
    "df_pb = pd.DataFrame(data)\n",
    "\n",
    "# Filter rows where City is 'Delhi' or 'Mumbai'\n",
    "filtered_df = df_pb[df_pb['City'].isin(['Delhi', 'Mumbai'])]\n",
    "\n",
    "# Display filtered DataFrame\n",
    "print(filtered_df)\n",
    "\n",
